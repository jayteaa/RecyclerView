<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ErgebnisDetailActivity"
    android:id="@+id/ergebnisDetailErg"
    >


    <ImageView
        android:id="@+id/LogoHeimmannschaftErg"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.115"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/LogoGastmannschaftErg"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_centerVertical="false"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.855"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/EndergebnisErg"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:layout_centerVertical="false"
        android:layout_marginTop="70dp"
        app:layout_constraintEnd_toStartOf="@+id/LogoGastmannschaftErg"
        app:layout_constraintStart_toEndOf="@+id/LogoHeimmannschaftErg"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/ZwischenergebnisErg"
        android:layout_width="60dp"
        android:layout_height="25dp"
        android:layout_centerVertical="false"
        android:layout_marginTop="100dp"
        app:layout_constraintEnd_toStartOf="@+id/LogoGastmannschaftErg"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toEndOf="@+id/LogoHeimmannschaftErg"
        app:layout_constraintTop_toTopOf="parent" />



    <TextView
        android:id="@+id/SpielstartErg"
        android:layout_width="358dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="description"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LogoHeimmannschaftErg" />

    <TextView
        android:id="@+id/StadionErg"
        android:layout_width="358dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="description"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SpielstartErg" />

    <TextView
        android:id="@+id/ZuschauerErg"
        android:layout_width="358dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="32dp"
        android:text="description"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/StadionErg" />


</androidx.constraintlayout.widget.ConstraintLayout>